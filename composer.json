{
    "name": "gurmanalexander/scout-elasticsearch-driver",
    "type": "library",
    "description": "The Elasticsearch Driver for Laravel Scout",
    "keywords": [
        "elastic",
        "elasticsearch",
        "driver",
        "engine",
        "laravel",
        "scout",
        "search"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "elasticsearch/elasticsearch": "^6.0",
        "laravel/scout": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "mockery/mockery": "1.*"
    },
    "autoload": {
        "psr-4": {
            "ScoutElastic\\Tests\\": "tests/",
            "ScoutElastic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScoutElastic\\ScoutElasticServiceProvider"
            ]
        }
    }
}
